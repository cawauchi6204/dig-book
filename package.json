{
  "name": "dig-book",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "rm -rf .next/cache && next dev -p 4000",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "gen-type": "mkdir -p types && source .env && npx supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > types/supabasetype.ts",
    "db:pull": "npx prisma db pull",
    "db:pull-print": "npx prisma db pull --print",
    "db:push": "npx prisma db push",
    "sync-supabase": "pnpm gen-type && pnpm db:pull"
  },
  "dependencies": {
    "@next/third-parties": "15.0.4-canary.4",
    "@prisma/client": "^5.22.0",
    "@react-spring/web": "^9.7.5",
    "@shadcn/ui": "^0.0.4",
    "@supabase/ssr": "^0.5.1",
    "@supabase/supabase-js": "^2.46.1",
    "@tanstack/react-query": "^5.60.5",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.456.0",
    "next": "14.2.5",
    "react": "19.0.0-rc-66855b96-20241106",
    "react-dom": "19.0.0-rc-66855b96-20241106",
    "react-tinder-card": "^1.6.4",
    "supabase": "^1.219.2",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.2",
    "@tanstack/eslint-plugin-query": "^5.60.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8",
    "eslint-config-next": "15.0.3",
    "postcss": "^8.4.47",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.14",
    "typescript": "^5"
  }
}
